<launch>
    <arg name="param_file" default="$(find strands_perception_people_utils)/config/detectors.yaml" />
    <rosparam command="load" file="$(arg param_file)"/>

    <arg name="machine" default="localhost" />
    <arg name="user" default="" />

    <machine name="$(arg machine)" address="$(arg machine)" env-loader="/opt/strands/strands_catkin_ws/devel/env.sh" user="$(arg user)" default="true"/>
    
    <node pkg="strands_perception_people_utils" type="to_pose_array.py" name="to_pose_array" output="screen">
    </node>

</launch> 
